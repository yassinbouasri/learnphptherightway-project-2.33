{
    "require": {
        "ext-pdo": "*",
        "vlucas/phpdotenv": "^5.3",
        "psr/container": "^2.0",
        "doctrine/dbal": "^4.2",
        "doctrine/orm": "^3.3",
        "symfony/cache": "^7.1",
      "ext-curl": "*",
        "illuminate/database": "^11.33",
        "guzzlehttp/guzzle": "^7.9",
        "twig/twig": "^3.0",
        "twig/intl-extra": "^3.15",
        "twig/extra-bundle": "^3.15",
      "ext-xdebug": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
